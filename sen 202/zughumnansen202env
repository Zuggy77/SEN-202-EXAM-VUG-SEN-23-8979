python -m venv zughumnan-sen202-env
zughumnan-sen202-env\Scripts\activate
pip install django djangorestframework
pip freeze > requirements.txt
# Ignore virtual environment
zughumnan-sen202-env/

# Ignore Python cache files
__pycache__/
*.pyc
*.pyo

# Ignore IDE settings
.vscode/
.idea/
git add requirements.txt .gitignore
git commit -m "Added virtual environment, dependencies, and gitignore"
git push
